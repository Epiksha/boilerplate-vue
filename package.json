{
    "name": "@epiksha/boilerplate-vue",
    "version": "1.0.0",
    "author": "Calvin Tiley",
    "description": "A boilerplate for Vue with Webpack, Babel and Sass.",
    "license": "ISC",
    "main": "index.js",
    "keywords": [
        "Vue",
        "Boilerplate",
        "Sass",
        "Webpack",
        "Babel",
        "Jest",
        "ESLint"
    ],
    "scripts": {
        "watch": "webpack serve --config webpack.development.js",
        "build": "webpack --config webpack.production.js",
        "build-dev": "webpack --config webpack.development.js",
        "setup": "npm ci",
        "test": "jest",
        "test:unit": "jest unit",
        "test:integration": "jest integration"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "resolutions": {
        "webpack": "5.49.0"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "vue": "^3.2.18",
        "vue-router": "^4.0.10",
        "vuex": "^4.0.2",
        "vuex-persistedstate": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "@vue/compiler-sfc": "^3.2.18",
        "babel-loader": "^8.2.2",
        "css-loader": "^6.3.0",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "cssnano": "^5.0.8",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.2",
        "postcss": "^8.3.8",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.1.1",
        "postcss-preset-env": "^6.7.0",
        "sass": "^1.42.1",
        "sass-loader": "^12.1.0",
        "sass-utilizer": "^3.2.7",
        "style-loader": "^3.3.0",
        "svg-sprite-loader": "^6.0.9",
        "svgo": "^2.7.0",
        "svgo-loader": "^3.0.0",
        "vue-loader": "^16.8.1",
        "vue-props-validation": "^0.1.1",
        "webpack": "^5.54.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.2.1",
        "webpack-merge": "^5.8.0"
    }
}
